<Window x:Class="TxtTranslate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TxtTranslate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label>Use this to translate some common abreviations into words</Label>
        <TextBox x:Name="txtbInput"></TextBox>
        <Button x:Name="btnTranslate" Click="btnTranslate_Click">Translate this</Button>
        <Label x:Name="lblOutput"></Label>
    </StackPanel>
</Window>
